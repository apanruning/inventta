{% if form.errors %}
{'form':[
    'errors': [{% for error in form.errors %}'{{error|safe}}', {%empty%}'No errors'{% endfor %}],
    {% for field in form %}
    {'{{field}}':[{% for error in field.errors %}'{{error|safe}}', {%empty%}'No errors'{% endfor %}}
    {% endfor %}        
    ]
}
{% else %}
{'message':'No hubo errores, no hubo excesos, son todos asesinos los milocos del proceso'}
{% endif %}
